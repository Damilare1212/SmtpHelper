<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <RootNamespace>MA.SmtpHelper</RootNamespace>
        <AssemblyName>MA.SmtpHelper</AssemblyName>
        <Title>Smtp Helper</Title>
        <Authors>Michael Armani</Authors>
        <Description>Chain of responsability like SMTP mail sender</Description>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageId>MichaelArmani.SmtpHelper</PackageId>
        <Copyright></Copyright>
        <PackageProjectUrl>https://github.com/armanimichael/SmtpHelper</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/armanimichael/SmtpHelper/blob/dev/LICENSE</PackageLicenseUrl>
        <RepositoryUrl>https://github.com/armanimichael/SmtpHelper</RepositoryUrl>
        <PackageTags>smtp, mail</PackageTags>
        <PackageVersion>1.0.1</PackageVersion>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="MailKit" Version="2.15.0" />
    </ItemGroup>

</Project>
